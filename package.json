{
  "name": "@pbelyaev/ydb-orm",
  "version": "0.0.1",
  "description": "Prisma-like ORM-ish client for YDB (YQL / table API)",
  "license": "MIT",
  "author": "Pavel Beliaev",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pbelyaev/ydb-orm.git"
  },
  "bugs": {
    "url": "https://github.com/pbelyaev/ydb-orm/issues"
  },
  "homepage": "https://github.com/pbelyaev/ydb-orm#readme",
  "keywords": [
    "ydb",
    "yql",
    "orm",
    "typescript"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@yandex-cloud/nodejs-sdk": "^2.9.0",
    "ydb-sdk": "^5.11.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
